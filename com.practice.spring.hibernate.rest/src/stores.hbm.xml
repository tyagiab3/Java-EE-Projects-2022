<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.practice.spring.hibernate.rest.classes.Stores"
		table="stores">
		<id name="store_id" column="store_id">
			<generator class="increment"></generator>
		</id>
		<property name="store_name" column="store_name"></property>
		<property name="phone" column="phone"></property>
		<property name="email" column="email"></property>
		<property name="street" column="street"></property>
		<property name="city" column="city"></property>
		<property name="state" column="state"></property>
		<property name="zip_code" column="zip_code"></property>
	</class>
</hibernate-mapping>
<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.practice.spring.hibernate.rest.classes.Staffs"
		table="staffs">
		<id name="staff_id" column="staff_id">
			<generator class="increment"></generator>
		</id>
		<property name="first_name" column="first_name"></property>
		<property name="last_name" column="last_name"></property>
		<property name="email" column="email"></property>
		<property name="phone" column="phone"></property>
		<property name="activity" column="activity"></property>
		<property name="store_id" column="store_id"></property>
		<property name="manager_id" column="manager_id"></property>
	</class>
</hibernate-mapping>